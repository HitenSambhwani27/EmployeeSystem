@model IEnumerable<December_Project.Dtos.BdgOut>
@{
    ViewData["Title"] = "BadgeOut";

}
<table class="table table-bordered">
<thead>
    <tr>
        <th>Photo</th>
        <th>Name</th>
        <th>Last Name</th>
        <th>Temproary Badge</th>
        
        <th>Assign</th>
    </tr>
</thead>
<tbody>
    @foreach (var data in Model)
    {
        <tr>
            <td>
                    <img src="~/images/@data.PhotoUrl" width="50">

            </td>
            <td>@data.employeefirstname</td>
             <td>@data.employeelastname</td>
            <td>@data.TemporaryBadge</td>
            
            <td> @data.SignIn</td>
            
            <td><button style="background-color:brown">
                <a href="/Home/Delete?Id=@data.employeefirstname&blobName=@data.employeelastname"   title="delete"> Remove </a>
                  
            </button>

            </td>

        </tr>
    }
</tbody>
</table>